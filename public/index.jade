          //Replace page styles (covers font, tile colours)
          if($div.find('#pagestyles').length){
          console.log('changing #pagestyles')
          $('#pagestyles').html( $div.find('#pagestyles').html() );
          }
          //Pull in the new Google webfont if applicable
          if( $div.find('#stream-font').length && $('#stream-font').length == 0) { //we just loaded a custom font but there's no stylesheet for it - make one
          console.log('adding new font stylesheet')
          $('#pagestyles').before( $div.find('#stream-font') );

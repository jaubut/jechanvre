        script.
          //Relative times on posts
          function updateTimeText(){
          console.log('updateTimeText()')
          $('.tile').each(function(){
          //read timestamp
          thisTimestamp =  $(this).attr('data-timestamp');
          timeNow = Math.round(new Date().getTime() / 1000);
          timediff = timeNow - thisTimestamp;
          //console.log(timediff);
          var timeText = ''
          if(isNaN(timediff)) { //if timediff was NaN then timeText will be too
          timeText = 'Moments ago';
          }
          else if(timediff >= 86400) { //dealing in days
          var daycount = Math.round(timediff / 60 / 60 / 24);
          timeText = (daycount == 1 ? '1 day ago' : daycount + ' days ago');
          }
          else if( timediff >= 3600) { //dealing in hours
          var hourvalue = Math.round(timediff / 60 / 60);
          timeText = (hourvalue == 1 ? '1 hour ago' : hourvalue + ' hours ago');
          }
          else if(timediff >= 60) { //dealing in minutes
          var minvalue = Math.round(timediff / 60 );
          timeText = (minvalue == 1 ? '1 minute ago' : minvalue + ' minutes ago');
          }
          else {
          timeText = 'Less than 1 minute ago';
          }
          $(this).find('.info .meta span').html(timeText);
          //$(this).children('.message').children('.messageInfo').children('.personTime').children('span').html(timeText);
          //console.log(timeText);
          });
          }//updateTimeText()
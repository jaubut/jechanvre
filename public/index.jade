          //used inside loadNewTile
          var colindexes = [];
          function refreshColIndexes() {
          //alert('refreshColindexes')
          colindexes = [];
          for(var i=1;i <= $('.matrix .column').length; i++) {
          colindexes.push(i);
          }
          colindexes = shuffle(colindexes);
          }
          refreshColIndexes();
          //Push a new tile onto the screen, into the next column in the queue
          function loadNewTile($post){
          console.log('Adding a tile of height ' + $post.height())
          colindex = colindexes.pop();
          if(colindexes.length == 0) {
          do {
          refreshColIndexes();
          }
          while((colindexes[0] != 'undefined') && (colindex == colindexes[0]) && colindexes.length > 1);
          }
          //find a list to put it in
          var $target = $('.column[data-index="' + colindex + '"]');
          //****************************************
          //'Smart' insert to maintain time order
          //****************************************
          var timestamp = $post.attr('data-timestamp');
          var inserted = false;
          //Figure out where to insert this post, and how.
          //If it's fresher than anything else in the $target column, we do the nice CSS animation and show it at the top
          //However if it needs to be inserted further down the column, or at the bottom, just slideDown()
          if($target.find('.tile').length) { //there are existing tiles, find the appropriate spot for this post
          //First a special check to see if the post is going to be at the top
          if(timestamp >= ($target.find('.tile').first().attr('data-timestamp') - 30)  || $post.attr('data-type') == 'advert' || $post.hasClass('pinned')) {
          $target.css('top','-'+($post.height()+20)+'px');
          $target.prepend($post);//remove dropzone
          $post.show();
          $target.animate({ 'top':0 }, 1000);
          }
          else {
          //not going at the top, so figure out where it is going
          $.each($target.find('.tile'), function(){
          $oldpost = $(this);
          if( $oldpost.attr('data-timestamp') < timestamp ) { //found a post which is older than the one we want to insert
          $post.css('display','none');
          $oldpost.before($post); //insert our post
          $post.hide().slideDown(1000); //show the post
          inserted = true; //flag that we are done inserting
          return false; //stop the $.each() loop for this tile
          }
          });
          //if we got to here and inserted is still false, this post goes on the bottom of the column
          if(inserted === false) {
          $target.append($post);
          $post.hide().slideDown(1000);
          }
          }
          }
          else { //first post in the column, just bung it in
          $target.css('top','-'+($post.height()+20)+'px');
          $target.prepend($post);//remove dropzone
          $post.show();
          $target.animate({ 'top':0 }, 1000);
          }
          //if we just inserted an ad, remove the last ad in the stream
          if($post.attr('data-type') == 'advert') {
          removeOldestAd();
          }
          if(typeof emptystream != undefined && emptystream != 0) {
          $('#emptyStream').remove();
          emptystream = 0;
          }
          }// loadNewTile()
          //An array shuffle function, because apparently that's not a thing in jquery
          //http://stackoverflow.com/a/2450976
          function shuffle(array) {
          var currentIndex = array.length, temporaryValue, randomIndex ;
          // While there remain elements to shuffle...
          while (0 !== currentIndex) {
          // Pick a remaining element...
          randomIndex = Math.floor(Math.random() * currentIndex);
          currentIndex -= 1;
          // And swap it with the current element.
          temporaryValue = array[currentIndex];
          array[currentIndex] = array[randomIndex];
          array[randomIndex] = temporaryValue;
          }
          return array;
          }//shuffle()
          function removeOldestAd() {
          if( $('.advertTile').length > ( Math.floor( 0.05 * $('.matrix .post').not('.advertTile').length ) + 1 ) ){
          console.log('found ' + $('.advertTile').length + ' ad tiles, should only have ' + Math.floor( 0.05 * $('.matrix .post').not('.advertTile').length ) + 1 );
          var oldest = '';
          $('.matrix .advertTile').each(function(){
          if(oldest == '' || $(this).attr('data-id') < oldest) {
          oldest = $(this).attr('data-id');
          }
          });
          if(oldest != '') {
          $('.matrix .advertTile[data-id="' + oldest + '"]').slideUp(2000,function(){ $(this).remove(); }); //slide post up and remove it
          }
          }
          }//removeOldestAd()
          });
        script.
          function updateStreamTheme($div) {
          if(typeof streamtheme != 'undefined') {
          console.log('updateStreamTheme()')
          var background = '';
          //background-color
          if(typeof streamtheme['background-color'] != 'undefined') {
          background += streamtheme['background-color'] + ' ';
          }
          //background-image
          if(typeof streamtheme['background-image'] != 'undefined') {
          background += ' url(' + streamtheme['background-image'] + ') left top/cover repeat-y fixed';
          }
          if(background != '') {
          $('html').css('background',background);
